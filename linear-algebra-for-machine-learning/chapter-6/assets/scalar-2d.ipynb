    "# broadcast scalar to two-dimensional array\n",
    "from numpy import array\n",
    "# define array\n",
    "A = array([\n",
    "\t[5, 10, 15],\n",
    "\t[10,20, 30]])\n",
    "print(A)\n",
    "# define scalar\n",
    "b = 3\n",
    "print(b)\n",
    "# broadcast\n",
    "c = A + b\n",
    "print(c)"
