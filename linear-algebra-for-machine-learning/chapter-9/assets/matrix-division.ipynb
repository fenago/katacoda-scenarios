    "# matrix division\n",
    "from numpy import array\n",
    "# define first matrix\n",
    "A = array([\n",
    "\t[10, 20, 30, 40, 50],\n",
    "\t[2, 4, 6, 8, 10]])\n",
    "print(A)\n",
    "# define second matrix\n",
    "B = array([\n",
    "\t[5, 10, 15, 20, 25],\n",
    "\t[3, 6, 9, 12, 15]])\n",
    "print(B)\n",
    "# divide matrices\n",
    "C = A / B\n",
    "print(C)"
   ]
